package com.cooksys.day_2;

public class Instrument {

	int numKeys;
	int range;
	
	
	@Override
	public String toString() {
		return "Instrument [numKeys=" + numKeys + ", range=" + range + "]";
	}
	
	@Override
	public int hashCode() {
		final int prime = 31;
		int result = 1;
		result = prime * result + numKeys;
		result = prime * result + range;
		return result;
	}
	
	@Override
	public boolean equals(Object obj) {
		if (this == obj)
			return true;
		if (obj == null)
			return false;
		if (getClass() != obj.getClass())
			return false;
		Instrument other = (Instrument) obj;
		if (numKeys != other.numKeys)
			return false;
		if (range != other.range)
			return false;
		return true;
	}

}
