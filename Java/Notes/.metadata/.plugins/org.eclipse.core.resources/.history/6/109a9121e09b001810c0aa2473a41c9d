package com.cooksys.day_4;

import java.io.File;
import java.util.Arrays;

/**
 * Hello world!
 *
 */
public class App 
{
    public static void main( String[] args )
    {
        File path = new File("./src");
        System.out.println(Arrays.toString(path.list()));
        System.out.println(path.isDirectory());
    }
}
