public class App 
{
    public static void main( String[] args )
    {
    	ParkingSpace space = new ParkingSpace();
        ParkingLot atriumIIParking = new ParkingLot("Atrium II Parking Lot");
        ParkingLot p1 = new ParkingLot("P1", space);
        System.out.println(p1.getName());
        System.out.println(atriumIIParking.getName());
        System.out.println(atriumIIParking.getSpace());
    }
}
